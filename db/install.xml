<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/iped/db" COMMENT="XMLDB file for Moodle mod/iped" VERSION="20101203">
  <TABLES>
    <TABLE NAME="iped" COMMENT="Associate moodle users to iped token">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Moodle user id"/>
        <FIELD NAME="token" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Iped Token"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>